    "author": "Renan Teixeira",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-cognito-identity-provider": "^3.908.0",
        "@aws-sdk/client-dynamodb": "^3.908.0",
        "@aws-sdk/client-s3": "^3.908.0",
        "@aws-sdk/client-ses": "^3.908.0",
        "@aws-sdk/lib-dynamodb": "^3.908.0",
        "aws-jwt-verify": "^5.1.1",
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^5.1.0",
        "jest": "^30.2.0",
        "jimp": "^1.6.0",
        "multer": "^2.0.2",
        "mysql2": "^3.15.2",
        "nodemailer": "^7.0.3",
        "qrcode": "^1.5.4",
        "sharp": "^0.34.4",
        "stripe": "^19.3.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "eslint": "^9.14.0",
        "nodemon": "^3.1.4"
    }
